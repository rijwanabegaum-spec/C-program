#include <stdio.h>

int main() {
    int a[5], b[5];
    int i;
    printf("Enter 5 elements for first array:\n");
    for(i = 0; i < 5; i++) {
        scanf("%d", &a[i]);
    }
    for(i = 0; i < 5; i++) {
        b[i] = a[i];
    }
    printf("\nFirst Array elements:\n");
    for(i = 0; i < 5; i++) {
        printf("%d ", a[i]);
    }
    printf("\nSecond Array elements:\n");
    for(i = 0; i < 5; i++) {
        printf("%d ", b[i]);
    }
    return 0;
}
