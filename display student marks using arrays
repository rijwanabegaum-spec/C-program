#include <stdio.h>

int main() {
    int n, i;
    int marks[100];

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter marks of student %d (out of 500): ", i + 1);
        scanf("%d", &marks[i]);
    }

    printf("\nMarks of students:\n");
    for (i = 0; i < n; i++) {
        printf("Student %d: %d / 500\n", i + 1, marks[i]);
    }

    return 0;
}
