#include <stdio.h>

struct Student {
    int id;
    char name[20];
};
int main() {
    struct Student s[10];
    int n, i;

    printf("Enter number of records: ");
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        printf("\nEnter details of student %d\n", i + 1);
        printf("ID: ");
        scanf("%d", &s[i].id);
        printf("Name: ");
        scanf("%s", s[i].name);
    }
    printf("\nTotal number of records stored = %d", n);
    return 0;
}
